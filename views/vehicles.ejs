<%- include("partials/header") -%>
           <title>Vehicles</title>
         </head>
       <body>

        <h1 class="text-danger mt-5"><center>Vehicles associated with <%= mycompany.companyName %></center></h1><hr class="bg-danger" style="width: 20%;">
        <section id="user_DashBoard">
            <div class="row r_1">
                <div class="col-lg-12 c_2">
                    <div class="menu_bar"><i class="fas fa-bars" id="hamburger"></i></div>
                        <div class="row c_2_r container-fluid" id="c_2_r_1">
                         <% vehicles.forEach((cmpnies) => { %>
                         <% cmpnies.forEach((cmp) => { %>
                             <div class="upper_cards col-lg-3">
                                 <div class="card">
                                     <div class="card-header">
                                         <img class="driver_pic" src="/company_driver_dp/ %> " >
                                     </div>
                                     <div class="card-body">
                                <p><%= cmp. modelName %> </p>
                                 <p> <%= cmp.vehicleNumber %> </p> 
                                  <p> <%= cmp.pollutionLevel  %> </p>    
                                      <p><%=cmp.insuranceDetailsNo %>  </p>
                                      <p><%= cmp.insuranceValidity  %>  </p>
                                           
                                  </div>
  
                                 </div>
                             </div>
                         <% }) %>
                         <% }) %>
                        </div>
                     </div>
               </div>
        </section>


<%- include("partials/footer") -%>